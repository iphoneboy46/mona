            .home_ab--box 
                .home_ab--box-lf(data-aos="fade-right")  
                    .home_ab--box-lf-wrap 
                        .home_ab--box-lf-box 
                            .about_food--box-rt-logo 
                                +img("abflogo.png")
                            .home_ab--box-lf-content 
