extends layout/layout
block vars
    -title = 'home'
    -mainClass = ''
    -bodyClass = ''
    -hdClass = 'BnBooking'
    -jsLibs = [JsJquery,JsAos,JsFancybox, JsSwiper,JsSplide,JsSplideAuto]
    -jsComponents = ['header',"home","scrollAni","booking","about","splideJS","popup"]
    -cssLibs = [fontAWS,fontAWS2,CssFancybox,CssSwiper,CssAos,CssSplide]
    -cssComponents = ['banner']
    -cssPages = ["home","booking","qc","contact","news","about"]

block container
    .banner_home 
        .banner_home--wrap
            .banner_home--content 
                p.note-text.cl-white.fw-7 Thời gian hoạt động
                p.note-text.cl-white 11:00 - 14:00 | 17:00 - 22:00, hằng ngày
            .banner_home--logo 
                +img("logoHome.png")
            .banner_home--list 
                .swiper.mySwiperBnHome 
                    .swiper-wrapper 
                        .swiper-slide
                            .banner_home--item 
                                .banner_home--item-wrap 
                                    .banner_home--item-img 
                                        +img("bnh1.png")
                        .swiper-slide
                            .banner_home--item 
                                .banner_home--item-wrap 
                                    .banner_home--item-img 
                                        +img("bnh2.png")
                        .swiper-slide
                            .banner_home--item 
                                .banner_home--item-wrap 
                                    .banner_home--item-img 
                                        +img("bnh3.png")
                .swiper-pagination.custom
    section.booking.home 
        .container 
            .booking_wrap 
                .booking_list 
                    .booking_item.ani 
                        .booking_item--wrap.d-wrap 
                            .booking_item--lf.d-item
                                .booking_item--lf-wrap 
                                    .booking_item--lf-list 
                                        .swiper.mySwiperBKLF 
                                            .swiper-wrapper 
                                                .swiper-slide
                                                    .booking_item--lf-item 
                                                        .booking_item--lf-img 
                                                            +img("bmn1.png")
                                                .swiper-slide
                                                    .booking_item--lf-item 
                                                        .booking_item--lf-img 
                                                            +img("bmn2.png")
                                                .swiper-slide
                                                    .booking_item--lf-item 
                                                        .booking_item--lf-img 
                                                            +img("bmn3.png")
                                        .swiper-pagination
                            .booking_item--rt.d-item
                                .booking_item--rt-wrap
                                    .booking_item--rt-box
                                        h2.title-sm3.cl-pri.fl-sec.fw-7 Thực Đơn
                                        .booking_item--rt-content 
                                            p Đến với Việt Nam, JUMBO Seafood vẫn giữ được vẹn nguyên công thức bí truyền nguyên bản từ Singapore, với mong muốn nâng tầm trải nghiệm ẩm thực đến với thực khách. Mọi hương vị tuyệt mỹ đều được tái hiện trọn vẹn thông qua bàn tay khéo léo, tài hoa của bếp trưởng người Singapore với hơn 30 năm kinh nghiệm trong nghề.
                                            br
                                            ul 
                                                li 11 phòng VIP
                                                li Phòng nhỏ nhất có sức chứa 6 Khách/ Phòng
                                                li Phòng lớn nhất có sức chứa tối đa 60 Khách/ Phòng
                                        a.btn(href="").trans
                                            span.btn-text Sơ đồ nhà hàng
    section.about_imgs.home 
        .about_imgs--wrap 
            .about_imgs--top 
                .container 
                    .about_imgs--top-wrap
                        .about_imgs--top-lf(data-aos="fade-right")
                            h2.title-sm3.fw-7.cl-pri.fl-sec Jumbo Seafood Vietnam
                        .about_imgs--top-rt(data-aos="fade-left") 
                            +box-navi("fas fa-chevron-left","fas fa-chevron-right")
            .about_imgs--bottom 
                .about_imgs--bottom-wrap(data-aos="fade-up") 
                    .about_imgs--bottom-list 
                        .swiper.mySwiperAbImgs 
                            .swiper-wrapper     
                                .swiper-slide 
                                    .about_imgs--bottom-item.vdjs 
                                        .about_imgs--bottom-item-wrap 
                                            button.btn-play.vdbtnjs
                                                .btn-play-tt
                                                .btn-play-btn
                                                    i.fas.fa-play
                                            .about_imgs--bottom-item-bg.vdbgjs
                                                +img("abi.png")
                                            .about_imgs--bottom-item-img
                                                video(width="320" height="240", controls)
                                                    source(src="./assets/images/food.mp4" type="video/mp4")
                                            .about_imgs--bottom-item-text 
                                                p.note-lg.t-center.cl-white 6A Trần Hưng Đạo, Phường Phạm Ngũ Lão, Quận 1, TP. HCM
                                .swiper-slide 
                                    .about_imgs--bottom-item.ytbjs 
                                        .about_imgs--bottom-item-wrap 
                                            button.btn-play.ytbbtnjs
                                                .btn-play-tt
                                                .btn-play-btn
                                                    i.fas.fa-play
                                            .about_imgs--bottom-item-bg.ytbbgjs
                                                +img("abi1.png")
                                            .about_imgs--bottom-item-img
                                                iframe(width="1280" height="720" id="youtubeIframe" src="https://www.youtube.com/embed/MiMaKBF8Us8?enablejsapi=1" title="Jumbo Seafood Vietnam" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" referrerpolicy="strict-origin-when-cross-origin", allowfullscreen)
                                            .about_imgs--bottom-item-text 
                                                p.note-lg.t-center.cl-white 6A Trần Hưng Đạo, Phường Phạm Ngũ Lão, Quận 1, TP. HCM
