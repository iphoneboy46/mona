    section.contact 
        .container 
            .contact_wrap.d-wrap 
                .contact_lf.d-item.d-2(data-aos="fade-right") 
                    .contact_lf--wrap 
                        .contact_lf--img 
                            +img("contactBg.png")
                .contact_rt.d-item.d-2(data-aos="fade-left") 
                    .contact_rt--wrap
                        .contact_rt--title 
                            h2.title-sm3.fw-7.cl-pri.fl-sec Liên Hệ Với Chúng Tôi
                            p.note-text.cl-gray6 Liên hệ ngay với chúng tôi để cùng hợp tác và phát triển, nơi mang đến những trải nghiệm ẩm thực tuyệt vời và dịch vụ chất lượng hàng đầu!
                        .contact_form
                            form
                                .contact_form--list.d-wrap
                                    .contact_form--item.d-item
                                        label.form-item
                                            input(type="text" placeholder="Họ và tên").form-item-ip
                                    .contact_form--item.d-item
                                        label.form-item
                                            input(type="text" placeholder="Số điện thoại").form-item-ip
                                    .contact_form--item.d-item
                                        label.form-item
                                            input(type="text" placeholder="Email").form-item-ip
                                    .contact_form--item.d-item
                                        label.form-item
                                            select.js-op.form-item-op(data-placeholder="Chi nhánh")
                                                option(value="")
                                                option(value="option1") demo 1
                                                option(value="option2") demo 2
                                                option(value="option3") demo 3
                                    .contact_form--item.d-item
                                        label.form-item
                                            select.js-op.form-item-op(data-placeholder="Nhu cầu")
                                                option(value="")
                                                option(value="option1") demo 1
                                                option(value="option2") demo 2
                                                option(value="option3") demo 3
                                    .contact_form--item.d-item
                                        label.form-item
                                            textarea(type="text" placeholder="Lời nhắn").form-item-tx
                                +btn("Gửi thông tin","","","trans")