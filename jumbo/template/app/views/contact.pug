extends layout/layout
block vars
    -title = 'contact'
    -mainClass = ''
    -bodyClass = ''
    -hdClass = ''
    -jsLibs = [JsJquery, JsSelect2,JsAos,JsSwiper]
    -jsComponents = ['header',"select2","popup","tab","contact"]
    -cssLibs = [fontAWS,fontAWS2,CssSelect2,CssAos,CssSwiper]
    -cssComponents = ['banner',"select2"]
    -cssPages = ["contact"]

block container
    +banner-main("bnNews.png","Liên Hệ")
    section.contact_map.tab-box 
        .container
            .contact_map-wrap(data-aos="zoom-out")  
                .contact_map--box 
                    .contact_map--lf
                        .contact_map--lf-wrap 
                            .contact_map--lf-list 
                                .contact_map--lf-item.tab-btn.actived 
                                    .contact_map--lf-item-wrap 
                                        h2.title-mn.fw-4.cl-white.fl-sec Jumbo Seafood Trần Hưng Đạo
                                        .address
                                            ul.address_list                
                                                li.address_item
                                                    .address_item--content
                                                        span.address_item--ic 
                                                            +img("mic1.png")
                                                        a.address_link(href="") 1900 292977
                                                li.address_item
                                                    .address_item--content
                                                        span.address_item--ic 
                                                            +img("mic2.png")
                                                        a.address_link(href="") 10:00 - 22:00, hằng ngày
                                                li.address_item
                                                    .address_item--content
                                                        span.address_item--ic 
                                                            +img("mic3.png")
                                                        a.address_link(href="") 6A Trần Hưng Đạo, Phường Phạm Ngũ Lão, Quận 1
                                .contact_map--lf-item.tab-btn 
                                    .contact_map--lf-item-wrap 
